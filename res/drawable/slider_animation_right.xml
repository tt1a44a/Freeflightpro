<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item
        n1:id="@android:id/background"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <corners
                n1:topLeftRadius="@dimen/animations_box_corners"
                n1:bottomLeftRadius="@dimen/animations_box_corners" />
            <solid
                n1:color="@android:color/transparent" />
            <stroke
                n1:width="1.0dip"
                n1:color="@android:color/transparent" />
        </shape>
    </item>
    <item
        n2:id="@android:id/progress"
        xmlns:n2="http://schemas.android.com/apk/res/android">
        <clip
            n2:gravity="right">
            <shape>
                <corners
                    n2:topLeftRadius="@dimen/animations_box_corners"
                    n2:bottomLeftRadius="@dimen/animations_box_corners" />
                <solid
                    n2:color="@color/transparent_white" />
            </shape>
        </clip>
    </item>
</layer-list>