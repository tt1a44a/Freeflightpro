<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item
        n1:state_selected="false"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <solid
                n1:color="@color/transparent_medium_dark_grey" />
            <stroke
                n1:width="1.0dip"
                n1:color="@color/transparent_light_grey" />
        </shape>
    </item>
    <item
        n2:state_selected="true"
        xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape>
            <stroke
                n2:width="2.0dip"
                n2:color="@color/green" />
            <solid
                n2:color="@color/transparent_medium_dark_grey" />
        </shape>
    </item>
</selector>