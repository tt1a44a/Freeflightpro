<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item
        n1:state_enabled="false"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <bitmap
            n1:src="@drawable/common_icn_flight_plan_options"
            n1:tint="@color/green" />
    </item>
    <item
        n2:state_pressed="false"
        xmlns:n2="http://schemas.android.com/apk/res/android">
        <bitmap
            n2:src="@drawable/common_icn_flight_plan_options"
            n2:tint="@android:color/white" />
    </item>
    <item
        n3:state_pressed="true"
        xmlns:n3="http://schemas.android.com/apk/res/android">
        <bitmap
            n3:src="@drawable/common_icn_flight_plan_options"
            n3:tint="@color/green" />
    </item>
</selector>