<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item
        n1:state_pressed="true"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <solid
                n1:color="@color/transparent_green" />
            <stroke
                n1:width="@dimen/button_stroke_width"
                n1:color="@color/green" />
            <corners
                n1:radius="@dimen/button_corner_radius" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                n2:color="@color/transparent_dark_green"
                xmlns:n2="http://schemas.android.com/apk/res/android" />
            <stroke
                n3:width="@dimen/button_stroke_width"
                n3:color="@color/green"
                xmlns:n3="http://schemas.android.com/apk/res/android" />
            <corners
                n4:radius="@dimen/button_corner_radius"
                xmlns:n4="http://schemas.android.com/apk/res/android" />
        </shape>
    </item>
</selector>