<?xml version="1.0" encoding="utf-8"?>
<merge
    n1:padding="30.0dip"
    n1:layout_width="fill_parent"
    n1:layout_height="fill_parent"
    xmlns:n1="http://schemas.android.com/apk/res/android">
    <ImageView
        n1:id="@id/image_action_icon"
        n1:layout_width="wrap_content"
        n1:layout_height="@dimen/timeline_cell_icon_height"
        n1:adjustViewBounds="true"
        n1:tint="@color/white" />
    <TextView
        n1:textColor="@color/white"
        n1:id="@id/text_action_values"
        n1:layout_width="wrap_content"
        n1:layout_height="wrap_content"
        n1:layout_marginTop="@dimen/timeline_cell_text_top_marge"
        n1:singleLine="true" />
</merge>