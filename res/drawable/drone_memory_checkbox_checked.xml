<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item>
        <shape
            n1:shape="rectangle"
            xmlns:n1="http://schemas.android.com/apk/res/android">
            <solid
                n1:color="@color/transparent_yellow" />
            <stroke
                n1:width="1.0dip"
                n1:color="@color/yellow" />
            <size
                n1:height="@dimen/drone_memory_checkbox_size"
                n1:width="@dimen/drone_memory_checkbox_size" />
            <corners
                n1:topLeftRadius="@dimen/drone_memory_checkbox_round_corner_size"
                n1:topRightRadius="@dimen/drone_memory_checkbox_round_corner_size"
                n1:bottomLeftRadius="@dimen/drone_memory_checkbox_round_corner_size"
                n1:bottomRightRadius="@dimen/drone_memory_checkbox_round_corner_size" />
        </shape>
    </item>
    <item
        n2:drawable="@drawable/ic_done_white_48dp"
        xmlns:n2="http://schemas.android.com/apk/res/android" />
</layer-list>