<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item
        n1:state_pressed="true"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <corners
                n1:topRightRadius="24.0dip"
                n1:bottomRightRadius="24.0dip" />
            <stroke
                n1:width="0.5dip"
                n1:color="@color/green" />
            <solid
                n1:color="@color/flightPlan_buttons_green_background" />
        </shape>
    </item>
    <item>
        <shape>
            <corners
                n2:topRightRadius="24.0dip"
                n2:bottomRightRadius="24.0dip"
                xmlns:n2="http://schemas.android.com/apk/res/android" />
            <stroke
                n3:width="0.5dip"
                n3:color="@color/piloting_button_outline_color"
                xmlns:n3="http://schemas.android.com/apk/res/android" />
            <solid
                n4:color="@color/flightPlan_buttons_green_background"
                xmlns:n4="http://schemas.android.com/apk/res/android" />
        </shape>
    </item>
</selector>