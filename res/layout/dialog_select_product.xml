<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    n1:gravity="center_horizontal"
    n1:orientation="vertical"
    n1:padding="@dimen/select_product_dialog_drone_padding"
    n1:layout_width="wrap_content"
    n1:layout_height="wrap_content"
    xmlns:n1="http://schemas.android.com/apk/res/android">
    <TextView
        n1:textSize="@dimen/dialog_title_text_size"
        n1:textStyle="bold"
        n1:textColor="@color/black"
        n1:id="@id/text_product_choice"
        n1:layout_width="wrap_content"
        n1:layout_height="wrap_content"
        n1:text="@string/product_choice"
        n1:textAllCaps="true" />
    <LinearLayout
        n1:gravity="center"
        n1:layout_width="wrap_content"
        n1:layout_height="wrap_content"
        n1:layout_marginTop="@dimen/select_product_dialog_drone_padding">
        <LinearLayout
            n1:gravity="center_horizontal"
            n1:orientation="vertical"
            n1:id="@id/layout_bebop_product"
            n1:background="@drawable/ripple_effect_round_corner"
            n1:clickable="true"
            n1:layout_width="@dimen/select_product_dialog_drone_size"
            n1:layout_height="wrap_content">
            <ImageView
                n1:duplicateParentState="true"
                n1:layout_width="@dimen/select_product_dialog_image_size"
                n1:layout_height="@dimen/select_product_dialog_image_size"
                n1:src="@drawable/sidebar_icn_product_0901" />
            <TextView
                n1:textSize="@dimen/select_product_dialog_text_size"
                n1:textColor="@color/black"
                n1:gravity="center"
                n1:id="@id/text_bebop_product"
                n1:layout_width="wrap_content"
                n1:layout_height="wrap_content"
                n1:maxLines="1" />
        </LinearLayout>
        <LinearLayout
            n1:gravity="center_horizontal"
            n1:orientation="vertical"
            n1:id="@id/layout_bebop2_product"
            n1:background="@drawable/ripple_effect_round_corner"
            n1:clickable="true"
            n1:layout_width="@dimen/select_product_dialog_drone_size"
            n1:layout_height="wrap_content">
            <ImageView
                n1:layout_width="@dimen/select_product_dialog_image_size"
                n1:layout_height="@dimen/select_product_dialog_image_size"
                n1:src="@drawable/sidebar_icn_product_090c" />
            <TextView
                n1:textSize="@dimen/select_product_dialog_text_size"
                n1:textColor="@color/black"
                n1:gravity="center"
                n1:id="@id/text_bebop2_product"
                n1:layout_width="wrap_content"
                n1:layout_height="wrap_content"
                n1:maxLines="1" />
        </LinearLayout>
        <LinearLayout
            n1:gravity="center_horizontal"
            n1:orientation="vertical"
            n1:id="@id/layout_chimera_product"
            n1:background="@drawable/ripple_effect_round_corner"
            n1:clickable="true"
            n1:layout_width="@dimen/select_product_dialog_drone_size"
            n1:layout_height="wrap_content">
            <ImageView
                n1:layout_width="@dimen/select_product_dialog_image_size"
                n1:layout_height="@dimen/select_product_dialog_image_size"
                n1:src="@drawable/sidebar_icn_product_0916"
                n1:tint="@color/black" />
            <TextView
                n1:textSize="@dimen/select_product_dialog_text_size"
                n1:textColor="@color/black"
                n1:gravity="center"
                n1:id="@id/text_chimera_product"
                n1:layout_width="wrap_content"
                n1:layout_height="wrap_content"
                n1:maxLines="1" />
        </LinearLayout>
        <LinearLayout
            n1:gravity="center_horizontal"
            n1:orientation="vertical"
            n1:id="@id/layout_evinrude_product"
            n1:background="@drawable/ripple_effect_round_corner"
            n1:clickable="true"
            n1:layout_width="@dimen/select_product_dialog_drone_size"
            n1:layout_height="wrap_content">
            <ImageView
                n1:layout_width="@dimen/select_product_dialog_image_size"
                n1:layout_height="@dimen/select_product_dialog_image_size"
                n1:src="@drawable/sidebar_icn_product_090e" />
            <TextView
                n1:textSize="@dimen/select_product_dialog_text_size"
                n1:textColor="@color/black"
                n1:gravity="center"
                n1:id="@id/text_evinrude_product"
                n1:layout_width="wrap_content"
                n1:layout_height="wrap_content"
                n1:maxLines="1" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>