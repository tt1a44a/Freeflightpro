<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    n1:gravity="center_horizontal"
    n1:orientation="vertical"
    n1:padding="@dimen/flightplan_edit_dialog_padding"
    n1:layout_width="wrap_content"
    n1:layout_height="wrap_content"
    xmlns:n1="http://schemas.android.com/apk/res/android">
    <TextView
        n1:textSize="@dimen/dialog_title_text_size"
        n1:id="@id/text_incorrect_password"
        n1:layout_width="wrap_content"
        n1:layout_height="wrap_content"
        n1:text="@string/wpa_password_error"
        n1:layout_centerHorizontal="true" />
    <TextView
        n1:textSize="@dimen/dialog_title_text_size"
        n1:gravity="center"
        n1:id="@id/text_password"
        n1:layout_width="wrap_content"
        n1:layout_height="wrap_content"
        n1:text="@string/wpa_password_8_chars_min__"
        n1:layout_below="@id/text_incorrect_password"
        n1:layout_centerHorizontal="true" />
    <EditText
        n1:textSize="@dimen/dialog_title_text_size"
        n1:layout_gravity="center"
        n1:id="@android:id/edit"
        n1:layout_width="fill_parent"
        n1:layout_height="wrap_content"
        n1:hint="@string/password"
        n1:layout_below="@id/text_password"
        n1:layout_alignStart="@id/text_password"
        n1:layout_alignEnd="@id/text_password"
        n2:layout_widthPercent="50.0%"
        xmlns:n2="http://schemas.android.com/apk/res-auto" />
</RelativeLayout>