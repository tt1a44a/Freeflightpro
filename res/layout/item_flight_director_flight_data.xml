<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    n1:layout_width="wrap_content"
    n1:layout_height="fill_parent"
    xmlns:n1="http://schemas.android.com/apk/res/android">
    <FrameLayout
        n1:id="@id/flight_data_view"
        n1:background="@drawable/flight_director_style_selector"
        n1:layout_width="wrap_content"
        n1:layout_height="wrap_content"
        n1:layout_centerVertical="true">
        <ImageView
            n1:id="@id/flight_data_image_view"
            n1:layout_width="wrap_content"
            n1:layout_height="wrap_content"
            n1:src="@drawable/btn_theme_cardwipe"
            n1:contentDescription="@null" />
        <TextView
            n1:textColor="@color/white"
            n1:gravity="center"
            n1:layout_gravity="center_vertical"
            n1:id="@id/flight_data_name"
            n1:layout_width="fill_parent"
            n1:layout_height="wrap_content"
            n1:src="@drawable/btn_theme_cardwipe"
            n1:contentDescription="@null" />
    </FrameLayout>
</RelativeLayout>