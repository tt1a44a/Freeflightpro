<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item
        n1:id="@android:id/progress"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <rotate
            n1:fromDegrees="270.0"
            n1:toDegrees="270.0"
            n1:pivotX="50.0%"
            n1:pivotY="50.0%">
            <shape
                n1:shape="ring"
                n1:innerRadiusRatio="2.5"
                n1:thicknessRatio="25.0">
                <solid
                    n1:color="@color/colorAccent" />
            </shape>
        </rotate>
    </item>
    <item
        n2:id="@android:id/secondaryProgress"
        xmlns:n2="http://schemas.android.com/apk/res/android">
        <rotate
            n2:fromDegrees="270.0"
            n2:toDegrees="270.0"
            n2:pivotX="50.0%"
            n2:pivotY="50.0%">
            <shape
                n2:shape="ring"
                n2:innerRadiusRatio="2.5"
                n2:thicknessRatio="25.0">
                <solid
                    n2:color="@color/background_grey" />
            </shape>
        </rotate>
    </item>
</layer-list>