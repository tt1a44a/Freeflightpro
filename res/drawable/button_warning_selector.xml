<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item
        n1:state_pressed="true"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape
            n1:shape="rectangle">
            <solid
                n1:color="@color/btn_warning_background_color" />
            <stroke
                n1:width="1.0dip"
                n1:color="@color/btn_warning_stroke_color" />
            <size
                n1:height="@dimen/settings_buttons_height" />
            <corners
                n1:topLeftRadius="@dimen/settings_buttons_half_height"
                n1:topRightRadius="@dimen/settings_buttons_half_height"
                n1:bottomLeftRadius="@dimen/settings_buttons_half_height"
                n1:bottomRightRadius="@dimen/settings_buttons_half_height" />
        </shape>
    </item>
    <item
        n2:state_pressed="false"
        xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape
            n2:shape="rectangle">
            <solid
                n2:color="@color/btn_warning_background_pressed_color" />
            <stroke
                n2:width="1.0dip"
                n2:color="@color/btn_warning_stroke_color" />
            <size
                n2:height="@dimen/settings_buttons_height" />
            <corners
                n2:topLeftRadius="@dimen/settings_buttons_half_height"
                n2:topRightRadius="@dimen/settings_buttons_half_height"
                n2:bottomLeftRadius="@dimen/settings_buttons_half_height"
                n2:bottomRightRadius="@dimen/settings_buttons_half_height" />
        </shape>
    </item>
</selector>