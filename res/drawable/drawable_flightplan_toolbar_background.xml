<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item
        n1:state_pressed="true"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <stroke
                n1:width="0.5dip"
                n1:color="@color/piloting_button_outline_color" />
            <solid
                n1:color="@color/flightPlan_buttons_green_background" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke
                n2:width="0.5dip"
                n2:color="@color/piloting_button_outline_color"
                xmlns:n2="http://schemas.android.com/apk/res/android" />
            <solid
                n3:color="@color/piloting_button_bg_color"
                xmlns:n3="http://schemas.android.com/apk/res/android" />
        </shape>
    </item>
</selector>