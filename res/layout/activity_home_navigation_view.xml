<?xml version="1.0" encoding="utf-8"?>
<merge>
    <android.support.design.widget.NavigationView
        n1:layout_gravity="start"
        n1:id="@id/home_drawer"
        n1:background="@color/home_navigation_background"
        n1:fitsSystemWindows="false"
        n1:layout_width="wrap_content"
        n1:layout_height="fill_parent"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <android.support.design.widget.NavigationView
            n1:layout_gravity="start"
            n1:id="@id/nav_view"
            n1:background="@android:color/transparent"
            n1:fitsSystemWindows="false"
            n1:layout_width="wrap_content"
            n1:layout_height="fill_parent"
            n1:layout_marginBottom="@dimen/navigation_view_margin_bottom"
            n2:headerLayout="@layout/layout_navigation_view_header"
            n2:itemIconTint="@color/green"
            n2:itemTextColor="@color/white"
            n2:menu="@menu/main_menu"
            xmlns:n2="http://schemas.android.com/apk/res-auto" />
        <TextView
            n1:textSize="@dimen/menu_nav_view_footer_text_size"
            n1:textColor="@color/menu_nav_view_footer_text_color"
            n1:layout_gravity="bottom"
            n1:id="@id/nav_view_app_version"
            n1:background="@android:color/transparent"
            n1:layout_width="fill_parent"
            n1:layout_height="wrap_content"
            n1:layout_marginBottom="@dimen/activity_vertical_margin"
            n1:text="@string/app_name"
            n1:layout_marginStart="@dimen/activity_horizontal_margin" />
    </android.support.design.widget.NavigationView>
</merge>