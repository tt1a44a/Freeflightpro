<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item>
        <shape
            n1:shape="rectangle"
            xmlns:n1="http://schemas.android.com/apk/res/android">
            <solid
                n1:color="@android:color/transparent" />
            <stroke
                n1:width="1.0dip"
                n1:color="@color/transparent_white" />
            <size
                n1:height="@dimen/settings_checkbox_size"
                n1:width="@dimen/settings_checkbox_size" />
            <corners
                n1:topLeftRadius="@dimen/settings_checkbox_half_size"
                n1:topRightRadius="@dimen/settings_checkbox_half_size"
                n1:bottomLeftRadius="@dimen/settings_checkbox_half_size"
                n1:bottomRightRadius="@dimen/settings_checkbox_half_size" />
        </shape>
    </item>
</layer-list>